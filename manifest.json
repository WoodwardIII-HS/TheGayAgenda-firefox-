{
  "name": "The Gay Agenda",
  "version": "0.1",
  "description": "Tucker Carlson's worst nightmare",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": [
        "jquery-3.5.1.min.js",
        "content.js"
      ],
      "css": [
        "themes.css",
        "content.css"
      ],
      "matches": [
        "https://calendar.google.com/*",
        "http://calendar.google.com/*"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
